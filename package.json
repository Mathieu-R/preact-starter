{
  "name": "webpack3-boilerplate",
  "version": "1.0.1",
  "description": "webpack3-boilerplate",
  "main": "src/index.js",
  "scripts": {
    "patch": "npm version bump",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack-dev-server --config build/webpack.config.js",
